# API Keys for various services
# Required for OpenAI API routes
OPENAI_API_KEY=your_openai_api_key_here

# Required for Google/Gemini API functionality
GOOGLE_API_KEY=your_google_api_key_here

# Required for image generation with Replicate
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Required for audio transcription with Deepgram
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Required for Anthropic/Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Firebase Configuration
# These need to be prefixed with NEXT_PUBLIC_ to be accessible in the browser
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id 